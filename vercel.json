{
  "headers": [
    {
      "source": "/(.*).gltf",
      "headers": [
        {
          "key": "Content-Type",
          "value": "model/gltf+json"
        }
      ]
    },
    {
      "source": "/(.*).bin",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        }
      ]
    }
  ]
}
